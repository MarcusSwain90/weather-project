var Library = function(){
  var books = [];
  
  var addBook = function(book){
    books.push(book)
  }
  return{
    add: addBook
  };
}

var library = Library(); 


var book = "Harry Potter";

library.add(book);


var Book = function() {
  var attributes = {
    title: "",
    author: ""
  } 
  
  var setAttribute = function(property, value){
		attributes[property] = value
  };

  var getAttributes = function(property){
		return attributes[property]
  };
  
  return{
    set: setAttribute,
    get: getAttributes
  }
};

var myBook = Book();



myBook.set('title', 'Harry Potter');
myBook.set('author', 'JK Rowling');


myBook.get('title'); // Harry Potter